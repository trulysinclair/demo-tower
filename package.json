{
  "name": "demo-tower",
  "version": "2.11.6",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "ace": "dotenvx run -- node ace",
    "build": "yarn ace build",
    "changelog": "auto-changelog",
    "db:setup": "yarn ace migration:run --force && yarn ace db:seed",
    "dev": "yarn db:setup && yarn ace serve --hmr",
    "format": "prettier --write .",
    "health-check": "yarn ace health-check",
    "lint": "eslint .",
    "postinstall": "husky",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "start": "dotenvx run -- node bin/server.js",
    "start:scheduler": "yarn ace scheduler:run",
    "test": "yarn ace test",
    "test:coverage": "c8 yarn ace test",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "@/*": "./intertia/lib/*",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#tmp/*": "./tmp/*",
    "#types/*": "./types/*.js",
    "#util/*": "./util/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@auto-it/all-contributors": "^11.1.6",
    "@auto-it/conventional-commits": "^11.1.6",
    "@auto-it/docker": "^11.1.6",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@ngrok/ngrok": "^1.3.0",
    "@swc/core": "^1.5.24",
    "@types/lodash": "^4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.13.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7",
    "@types/xml2js": "^0",
    "@vitejs/plugin-react": "^4.3.1",
    "auto": "^11.1.6",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.19",
    "c8": "^10.1.2",
    "eslint": "^8.57.0",
    "hot-hook": "^0.2.6",
    "husky": "^9.0.11",
    "nock": "^14.0.0-beta.7",
    "pino-pretty": "^11.1.0",
    "pinst": "^3.0.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.0",
    "semver": "^7.6.2",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.0.2",
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.12.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^1.1.0",
    "@adonisjs/lucid": "^21.1.1",
    "@adonisjs/mail": "^9.2.2",
    "@adonisjs/session": "^7.4.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/transmit": "^1.0.2",
    "@adonisjs/vite": "^3.0.0",
    "@dotenvx/dotenvx": "^1.6.4",
    "@faker-js/faker": "^8.4.1",
    "@inertiajs/react": "^1.2.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-label": "^2.1.0",
    "@react-email/components": "^0.0.22",
    "@react-email/render": "^0.0.17",
    "@tiotbenjy/ally-entra-id": "^1.0.1",
    "@vinejs/vine": "^2.1.0",
    "adonisjs-scheduler": "^1.0.5",
    "better-sqlite3": "^11.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "edge.js": "^6.0.2",
    "execa": "^9.3.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.408.0",
    "luxon": "^3.4.4",
    "openapi-fetch": "^0.10.2",
    "openapi-typescript": "^7.3.0",
    "pino-roll": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "auto-changelog": {
    "commitLimit": false,
    "package": true,
    "output": "changelog.md"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app",
    "ignorePatterns": [
      "types/*.schema.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "packageManager": "yarn@4.3.0"
}
